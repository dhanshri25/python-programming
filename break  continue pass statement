av = 5
x = int(input("How many Candies you want"))
i = 1
while i <= x:
    if i > av:
        print("OUT OF STOCK")
        break
    print("candy")
    i = i + 1
print("Bye")

#example 2
for i in range(0,16):
    if i % 3 == 0 and i % 5 == 0:
        continue
    print(i)
print("bye")


#example 3
for i in range(0,10):
    if i % 2 != 0:
        pass
    else:
        print(i)
print("bye")