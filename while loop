#example 1
i = 1

while i <= 5:
    print('Telegram')
    i = i + 1

#example 2
i = 5

while i >= 1:
    print(i)
    i = i - 1


#example 3
i = int(input('Enter a number'))

while 1 >= i:
    print(i)
    i = i - 1

#example 4
i=1
while i<=5:
    print("Telegram",end="")
    j=1
    while j <= 4:
        print('Whatsapp',end="")
        j = j + 1
    i = i + 1
    print()